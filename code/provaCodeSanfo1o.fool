class Numeri (int vala, int valb){

    void setNumeri(int a,int b) {
        vala=10;
        valb=10;
    }

    int getNumeroA() {
        return vala+1;
    }

    int getNumeroB() {
        return valb+2;
    }
}

void main(){
    let
        Numeri n = new Numeri();
    in
        n.setNumeri(1,2);
        print(n.getNumeroA());
        print(n.getNumeroB());
}