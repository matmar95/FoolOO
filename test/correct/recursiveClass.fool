class B(int a, int b){
    B(int aa,int bb){
        a=aa;
        b=bb;
        print(a);
        print(b);
    }

    int dec(int aa){
        if(aa>=0) then {
            if(aa==2) then{
                a=1;
                b=-1;
                print(a);
                print(b);
            }
            if(aa==1) then {
                a=2;
                b=-2;
                print(a);
                print(b);
            }
            if(aa==0) then {
                a=3;
                b=-3;
                print(a);
                print(b);
            }
            aa = this.dec(aa-1);
            print(aa);
        }
        return aa;
    }
}

void main(){
    let
        B b = new B(3, 4);
        int aaaaa=7;
    in
        aaaaa = b.dec(2);
        print(aaaaa);
}

