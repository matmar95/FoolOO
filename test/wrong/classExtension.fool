class A (int a, int b){
    void setA(int aa){
        a=aa;
    }

    void setB(int bb){
        b=bb;
    }

    int getA(){
        return a;
    }

    int getB(){
        return b;
    }
}

class B extends A(int a, int b){
    int raddoppiaA(){
        return a*2;
    }

    int raddoppiaB(){
        return b*2;
    }
}

void main(){
    let
        B b = new B();
    in
        b.setA(2);
        b.setB(4);
        print(b.raddoppiaA());
        print(b.raddoppiaB());
}